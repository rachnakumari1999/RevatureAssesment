package weekencap;

public class Inheritance 
{
	double length=10;
	double breadth=20;
	double height=30;
}
class Square extends Inheritance{
	void area(){
		System.out.println("Area of square:"+length*length);
		 
	}
	
   public static void main(String[] args)
   {
		// TODO Auto-generated method stub
		Square s=new Square();
		s.area();
