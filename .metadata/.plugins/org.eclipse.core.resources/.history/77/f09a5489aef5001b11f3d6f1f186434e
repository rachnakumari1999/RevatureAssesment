package weekencap;

class Shape 
{
	double length=10;
	double breadth=20;
	double height=30;
}
class Square extends Shape {
	void area(){
		System.out.println("Area of square:"+length*length);
		 
	}
public class Inheritance
{
	
	public static void main(String[] args)
   {
		// TODO Auto-generated method stub
		Square s=new Square();
		s.area();
		//System.out.println("hii");
	}
}

